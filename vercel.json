{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "index.js"
    },
    {
      "src": "/users/.*",
      "dest": "./src/routes/users.js"
    },
    {
      "src": "/auth/.*",
      "dest": "./src/routes/auth.js"
    }
  ]
}
