{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "./index.js"
    },
    {
      "src": "/users/(.*)",
      "dest": "./index.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "./index.js"
    },
    {
      "src": "/login/(.*)",
      "dest": "./index.js"
    },
    {
      "src": "/logout/(.*)",
      "dest": "./index.js"
    }
  ]
}
